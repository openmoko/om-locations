group {
   /* the main object here */
   name: "modules/diversity_nav/main";
   parts {
      part { name: "zoom_in";
         mouse_events: 1;
         description { state: "default" 0.0;
            min: 48 48;
            rel1 {
               relative: 0.0 0.0;
               offset: -1 -1;
            }
            rel2 {
               relative: 0.0 0.0;
               offset: -1 -1;
            }
            align: 0.0 0.0;
            image.normal: "zoomin.png";
         }
      }
      part { name: "zoom_out";
         mouse_events: 1;
         description { state: "default" 0.0;
            min: 48 48;
            rel1 {
               relative: 0.0 1.0;
               offset: 0 -48;
            }
            rel2 {
               relative: 0.0 1.0;
               offset: 0 -48;
            }
            align: 0.0 1.0;
            image.normal: "zoomout.png";
         }
      }
      part { name: "up";
         mouse_events: 1;
         description { state: "default" 0.0;
            min: 48 48;
            rel1 {
               to: "down";
               relative: 0.0 0.0;
               offset: 0 -48;
            }
            rel2 {
               to: "down";
               relative: 0.0 0.0;
               offset: 0 -1;
            }
            align: 0.0 1.0;
            image.normal: "up.png";
         }
      }
      part { name: "left";
         mouse_events: 1;
         description { state: "default" 0.0;
            min: 48 48;
            max: 48 48;
            rel1 {
               to: "down";
               relative: 0.0 0.0;
               offset: -144 0;
            }
            rel2 {
               to: "down";
               relative: 0.0 1.0;
               offset: -1 0;
            }
            align: 0.0 1.0;
            image.normal: "left.png";
         }
      }
      part { name: "right";
         mouse_events: 1;
         description { state: "default" 0.0;
            min: 48 48;
            rel1 {
               to: "down";
               relative: 1.0 0.0;
               offset: 96 0;
            }
            rel2 {
               to: "down";
               relative: 1.0 1.0;
               offset: 0 0;
            }
            align: 0.0 1.0;
            image.normal: "right.png";
         }
      }
      part { name: "down";
         mouse_events: 1;
         description { state: "default" 0.0;
            min: 48 48;
            rel1 {
               relative: 0.5 1.0;
               offset: 0 -48;
            }
            rel2 {
               relative: 0.5 1.0;
               offset: 0 -48;
            }
            align: 0.5 1.0;
            image.normal: "down.png";
         }
      }
      part { name: "zoom_base";
         type: RECT;
         mouse_events: 0;
         description {
            state: "default" 0.0;
            color: 0 0 0 0;
            min: 48 0;
            align: 0.0 0.5;
            rel1 {
               offset: 0 48;
            }
            rel2 {
               to: "zoom_out";
               relative: 0.0 0.0;
               offset: 0 -1;
            }
         }
      }
      part {
         name: "e.dragable.zoom";
         mouse_events: 1;
         dragable {
            x: 0 0 0;
            y: 1 1 0;
            confine: "zoom_base";
         }
         description {
            state: "default" 0.0;
            min: 48 48;
            fixed: 1 1;
            rel1 {
               relative: 0.5  0.5;
               offset:   0    0;
               to: "zoom_base";
            }
            rel2 {
               relative: 0.5  0.5;
               offset:   0    0;
               to: "zoom_base";
            }
            image.normal: "zoom.png";
         }
      }
      part { name: "zoom_length";
         mouse_events: 0;
         description { state: "default" 0.0;
            min: 80 16;
            max: 80 16;
            rel1 {
               to: "e.text.zoom";
               relative: 0.0 0.0;
               offset: -1 -1;
            }
            rel2 {
               to: "e.text.zoom";
               relative: 0.0 0.0;
               offset: -1 -1;
            }
            align: 0.0 1.0;
            image.normal: "measure_64px.png";
         }
      }
      part { name: "e.text.zoom";
         type: TEXT;
         effect: SOFT_SHADOW;
         mouse_events: 0;
         description { state: "default" 0.0;
            rel1 {
               relative: 0.0 1.0;
               offset: 0 -1;
            }
            rel2 {
               relative: 0.0 1.0;
               offset: 0 -1;
            }
            align: 0.0 1.0;
            color: 255 255 255 255;
            color3: 0 0 0 32;
            text {
               font: "Sans:style=Bold,Edje-Vera-Bold";
               size: 10;
               min: 1 1;
               align: 0.0 0.0;
            }
         }
      }
      part { name: "e.text.latitude";
         type: TEXT;
         effect: SOFT_SHADOW;
         mouse_events: 0;
         description { state: "default" 0.0;
            rel1 {
               to: "e.text.longitude";
               relative: 1.0 0.0;
               offset: -1 -1;
            }
            rel2 {
               to: "e.text.longitude";
               relative: 1.0 0.0;
               offset: -1 -1;
            }
            align: 1.0 1.0;
            color: 255 255 255 255;
            color3: 0 0 0 32;
            text {
               font: "Sans:style=Bold,Edje-Vera-Bold";
               size: 10;
               min: 1 1;
               align: 0.0 0.0;
            }
         }
      }
      part { name: "e.text.longitude";
         type: TEXT;
         effect: SOFT_SHADOW;
         mouse_events: 0;
         description { state: "default" 0.0;
            rel1 {
               relative: 1.0 1.0;
               offset: -1 -1;
            }
            rel2 {
               relative: 1.0 1.0;
               offset: -1 -1;
            }
            align: 1.0 1.0;
            color: 255 255 255 255;
            color3: 0 0 0 32;
            text {
               font: "Sans:style=Bold,Edje-Vera-Bold";
               size: 10;
               min: 1 1;
               align: 0.0 0.0;
            }
         }
      }
   }
   programs { /* programs - where we react to events/signals */
       
   }
}
