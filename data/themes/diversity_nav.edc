group {
   name: "modules/diversity_nav/taglist";
   parts {
      part { name: "background";
         type: RECT;
         description {
            state: "default" 0.0;
            rel1.offset: 10 10;
            rel2.offset: -10 -10;
            color: 0 0 0 255;
         }     
      }
      part { name: "swallow";
         type: SWALLOW;
         description {
            state: "default" 0.0;
            rel1.offset: 10 10;
            rel1.to:     "background";
            rel2.offset: -10 -10;
            rel2.to:     "background";
         } 
      }
   }
}

group {
   name: "modules/diversity_nav/textedit";
   parts {
      part { name: "background";
         type: RECT;
         description {
            state: "default" 0.0;
            rel1.offset: 10 10;
            rel2.offset: -10 -10;
            color: 0 0 0 255;
         }     
      }
      
      part { name: "title";
         type: TEXT;
         mouse_events: 0;
         description {
            state: "default" 0.0;
            rel1 {
               relative: 0.0 0.0;
               offset: 0 0;
               to: "background";
            }
            rel2 {
               relative: 1.0 0.3;
               offset: 0 0;
               to: "background";
            }
            align: 0.0 0.0;
            color: 255 255 255 255;
            text {
               font: "Sans:style=Bold,Edje-Vera-Bold";
               size: 20;
               min: 1 1;
               align: 0.0 0.0;
            }
         }
      }

      part { name: "swallow";
         type: SWALLOW;
         description {
            state: "default" 0.0;
            rel1 {
               relative: 0.0 0.3;
               offset: 10 10;
               to: "background";
            }
            rel2 {
               relative: 1.0 1.0;
               offset: -10 -10;
               to: "background";
            }
         } 
      }
   }
}

