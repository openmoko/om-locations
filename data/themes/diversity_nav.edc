         }
      }
      part { name: "e.text.name";
         type: TEXTBLOCK;
         effect: SOFT_SHADOW;
         mouse_events: 1;
         description { state: "default" 0.0;
	    fixed: 1 1;
            rel1 {
               to: "e.image.location";
               relative: 0.7 0.25;
               offset: 0 0;
            }
            rel2 {
               to: "e.image.location";
               relative: 1.0 0.25;
               offset: 300 0;
            }
            align: 0.0 0.0;
            visible: 0;
            color: 0 0 0 255;
            color3: 0 0 0 0;
            text {
               style:    "location_textblock_style";
               min:      0 1;
            }
         }
         description { state: "visible" 0.0;
            inherit: "default" 0.0;
            visible: 1;
            color: 0 0 0 255;
            color3: 0 0 0 0;
         }
      }
      part { name: "e.text.name2";
         type: TEXT;
         effect: SOFT_SHADOW;
         mouse_events: 1;
         description { state: "default" 0.0;
	    fixed: 1 1;
            rel1 {
               to: "e.text.name";
               relative: 0.0 1.0;
               offset: 0 -8;
            }
            rel2 {
               to: "e.text.name";
               relative: 1.0 1.0;
               offset: 0 0;
            }
            align: 0.0 0.0;
            visible: 0;
            color: 33 33 33 255;
            color3: 0 0 0 0;
            text {
               font: "Edje-Sans-Bold";
               size: 16;
               min: 1 1;
               align: 0.0 0.0;
            }
         }
         description { state: "visible" 0.0;
            inherit: "default" 0.0;
            visible: 1;
            color: 33 33 33 255;
            color3: 0 0 0 0;
         }
      }
   }
   programs { /* programs - where we react to events/signals */
      program {
         name: "appear";
         signal: "e,state,active";
         source: "e";
         action: STATE_SET "visible" 0.0;
         transition: DECELERATE 0.0;
         target: "e.text.name";
         target: "e.text.name2";
      }
      program {
         name: "disappear";
         signal: "e,state,passive";
         source: "e";
         action: STATE_SET "default" 0.0;
         transition: DECELERATE 0.0;
         target: "e.text.name";
         target: "e.text.name2";
      }
      program {
         name: "menu_activate";
         signal: "mouse,down,*";
         source: "e.text.name";
         action, SIGNAL_EMIT "MENU_ACTIVATE" "e.text.name"; 
      }
      program {
         name: "menu_activate2";
         signal: "mouse,down,*";
         source: "e.text.name2";
         action, SIGNAL_EMIT "MENU_ACTIVATE" "e.text.name"; 
      }
   }
}

