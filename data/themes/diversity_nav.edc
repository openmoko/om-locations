group {
   name: "modules/diversity_nav/message";
   parts {
      part { name: "message.icon";
         mouse_events: 0;
         description { state: "default" 0.0;
            min: 16 16;
            max: 16 16;
            rel1 {
               relative: 0.0 0.0;
               offset: 0 0;
            }
            rel2 {
               relative: 1.0 1.0;
               offset: 0 0;
            }
            align: 0.0 0.0;
            fill.smooth: 0;
            image.normal: "red_dot1.png";
         }
         description { state: "red0" 0.0;
            inherit: "default" 0.0;
            color: 255 255 255 255;
         }
         description { state: "red1" 0.0;
            inherit: "default" 0.0;
            color: 255 255 255 128;
         }
         description { state: "red2" 0.0;
            inherit: "default" 0.0;
            color: 255 255 255 64;
         }
         description { state: "red3" 0.0;
            inherit: "default" 0.0;
            color: 255 255 255 32;
         }
      }
      part { name: "message.text";
         type: TEXT;
         mouse_events: 0;
         description {
            state: "default" 0.0;
	    fixed: 1 1;
            rel1 {
               relative: 1.0 0.0;
               offset: 5 0;
               to: "message.icon";
            }
            rel2 {
               relative: 1.0 1.0;
               offset: 5 0;
               to: "message.icon";
            }
            align: 0.0 0.5;
            color: 0 0 0 255;
            text {
               font: "Edje-Sans-Bold";
               size: 16;
               min: 1 1;
               align: 0.0 0.0;
            }
         }
      }
   }
   programs { /* programs - where we react to events/signals */
      program {
         name: "show1";
         signal: "show";
         action: STATE_SET "default" 0.0;
         transition: LINEAR 1.0;
         target: "message.icon";
         after: "show2";
      }
      program {
         name: "show2";
         action: STATE_SET "red1" 0.0;
         transition: LINEAR 0.5;
         target: "message.icon";
         after: "show3";
      }
      program {
         name: "show3";
         action: STATE_SET "red2" 0.0;
         transition: LINEAR 0.5;
         target: "message.icon";
         after: "show4";
      }
      program {
         name: "show4";
         action: STATE_SET "red3" 0.0;
         transition: LINEAR 0.5;
         target: "message.icon";
         after: "show1";
      }
   }
}

group {
   name: "modules/diversity_nav/list";
   parts {
      part { name: "background";
         type: RECT;
         description {
            state: "default" 0.0;
            rel1.offset: 0 0;
            rel2.offset: 0 0;
            color: 0 0 0 255;
         }     
      }
      part { name: "button_bar";
         type: SWALLOW;
         description { state: "default" 0.0;
            color: 255 255 255 255;
	    visible: 0;
	    rel1 {
		to: "background";
	    	relative: 0.0 0.0;
		offset: 0 0;
	    }
            rel2 {
               to: "background";
               relative: 1.0 0.0;
               offset: -30 39;
            }
         }
         description { state: "shown" 0.0;
	    inherit: "default" 0.0;
	    visible: 1;
	    rel2.offset: -30 85;
         }
      }
      part { name: "swallow";
         type: SWALLOW;
         description {
            state: "default" 0.0;
	    rel1 {
	       to_y: "button_bar";
	       relative: 0.0 1.0;
	       offset: 10 0;
	    }
	    rel2 {
	       to: "background";
	       offset: -10 0;
	    }
         }
         description {
            state: "taglist" 0.0;
	    inherit: "default" 0.0;
	    rel2.offset: -10 -40;
         }
      }
      part { name: "title";
         type: TEXT;
         mouse_events: 0;
         description {
            state: "default" 0.0;
            rel1 {
               to: "swallow";
               relative: 0.0 0.0;
               offset: 3 0;
            }
            rel2 {
	       to: "swallow";
               relative: 0.0 0.0;
               offset: 3 -10;
            }
            align: 0.0 1.0;
            color: 255 255 255 255;
            text {
               font: "Edje-Sans-Bold";
               size: 16;
               min: 1 1;
               align: 0.0 0.5;
            }
         }
      }
   }
   programs {
      program { 
         name: "on";
         signal: "e,state,active";
         source: "e";
         action: STATE_SET "shown" 0.0;
         target: "button_bar";
      }
      program { 
         name: "off";
         signal: "e,state,passive";
         source: "e";
         action: STATE_SET "default" 0.0;
         target: "button_bar";
      }
      program {
         name: "list.tag";
         signal: "e,state,taglist";
         source: "e";
         action: STATE_SET "taglist" 0.0;
         target: "swallow";
      }
   }
}

