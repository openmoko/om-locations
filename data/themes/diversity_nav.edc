group {
   name: "modules/diversity_nav/access_point";
   parts {
      part { name: "range";
         mouse_events: 0;
         description { state: "default" 0.0;
            image.normal: "ap_range.png";
         }
      }
      part { name: "tower";
         description { state: "default" 0.0;
            min: 48 48;
            max: 48 48;
            image.normal: "ap_tower.png";
         }
      }
      part { name: "s0";
         description { state: "default" 0.0;
            rel1 { 
               to: "top";
               relative: 0.0 0.0;
            }
            rel2 {
               to: "top";
               relative: 1.0 1.0;
            }
            image.normal: "ap_signal.png";
            fill.smooth: 0;
            color: 255 255 255 255;
         }
         description { state: "out" 0.0;
            inherit: "default" 0.0;
            rel1.relative: -0.5 -0.5;
            rel2.relative: 1.5 1.5;
            color: 255 255 255 128;
         }
      }
      part { name: "s1";
         description { state: "default" 0.0;
            rel1 { 
               to: "top";
               relative: -0.5 -0.5;
            }
            rel2 {
               to: "top";
               relative: 1.5 1.5;
            }
            image.normal: "ap_signal.png";
            fill.smooth: 0;
            color: 255 255 255 128;
         }
         description { state: "out" 0.0;
            inherit: "default" 0.0;
            rel1.relative: -1.0 -1.0;
            rel2.relative: 2.0 2.0;
            color: 255 255 255 64;
         }
      }
      part { name: "s2";
         description { state: "default" 0.0;
            rel1 { 
               to: "top";
               relative: -1.0 -1.0;
            }
            rel2 {
               to: "top";
               relative: 2.0 2.0;
            }
            image.normal: "ap_signal.png";
            fill.smooth: 0;
            color: 255 255 255 64;
         }
         description { state: "out" 0.0;
            inherit: "default" 0.0;
            rel1.relative: -1.5 -1.5;
            rel2.relative: 2.5 2.5;
            color: 255 255 255 32;
         }
      }
      part { name: "s3";
         description { state: "default" 0.0;
            rel1 { 
               to: "top";
               relative: -1.5 -1.5;
            }
            rel2 {
               to: "top";
               relative: 2.5 2.5;
            }
            image.normal: "ap_signal.png";
            fill.smooth: 0;
            color: 255 255 255 32;
         }
         description { state: "out" 0.0;
            inherit: "default" 0.0;
            rel1.relative: -2.0 -2.0;
            rel2.relative: 3.0 3.0;
            color: 255 255 255 0;
         }
      }
      part { name: "top";
         description { state: "default" 0.0;
            min: 10 10;
            max: 10 10;
            rel1 {
               to: "tower";
               offset: 19 12;
            }
            rel2 {
               to: "tower";
               relative: 0.0 0.0;
               offset: 28 21;
            }
            image.normal: "ap_top.png";
         }
      }
      part { name: "e.text.name";
         type: TEXT;
         effect: SOFT_SHADOW;
         mouse_events: 0;
         description { state: "default" 0.0;
            rel1 {
               to_y: "tower";
               relative: 0.0 1.0;
               offset: 0 0;
            }
            rel2 {
               to_y: "tower";
               relative: 1.0 1.0;
               offset: -1 0;
            }
            align: 0.5 0.0;
            color: 255 255 255 255;
            color3: 0 0 0 32;
            text {
               font: "Sans:style=Bold,Edje-Vera-Bold";
               size: 10;
               min: 1 1;
               align: 0.5 0.0;
            }
         }
      }
   }
   programs { /* programs - where we react to events/signals */
      program {
         name: "show1";
         signal: "show";
         source: "";
         action: STATE_SET "out" 0.0;
         transition: LINEAR 0.5;
         target: "s0";
         target: "s1";
         target: "s2";
         target: "s3";
         after: "show2";
      }
      program {
         name: "show2";
         action: STATE_SET "default" 0.0;
         target: "s0";
         target: "s1";
         target: "s2";
         target: "s3";
         after: "show1";
      }
   }
}

