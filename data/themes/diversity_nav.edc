styles 
{
   style {
      name: "location_textblock_style";
      base: "font=Edje-Sans-Bold font_size=28 text_class=tb_plain align=left color=#fff style=shadow shadow_color=#ffffff00 wrap=word";

      tag:  "br" "\n";
